   "source": [
    "import socket\n",
    "\n",
    "msg = \"Hello Server\"\n",
    "bytes_tx=str.encode(msg)\n",
    "\n",
    "server_address = (\"127.0.0.1\",6780)\n",
    "socket = socket.socket(family=socket.AF_INET, type=socket.SOCK_DGRAM)\n",
    "socket.sendto(bytes_tx, server_address)\n",
    "\n",
    "bytes_rx = socket.recvfrom(1024)\n",
    "print(\"RX: \",bytes_rx)\n",
    "socket.close()\n"
   ]
  }
